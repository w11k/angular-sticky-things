{
  "name": "angular-sticky-things-demo",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "prestart": "npm run build:lib",
    "build": "ng build --configuration production --base-href /angular-sticky-things/",
    "build:lib": "ng build angular-sticky-things --configuration production",
    "test": "ng test angular-sticky-things-demo --watch=false",
    "test:lib": "ng test angular-sticky-things --watch=false",
    "test:lib:watch": "ng test angular-sticky-things",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-watch": "concurrently --kill-others \"npm start\" \"npm run e2e-watch-no-serve\"",
    "e2e-watch-no-serve": "watch \"npm run e2e -- --dev-server-target=\" src e2e --wait=1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.9",
    "@angular/common": "^13.3.9",
    "@angular/compiler": "^13.3.9",
    "@angular/core": "^13.3.9",
    "@angular/forms": "^13.3.9",
    "@angular/platform-browser": "^13.3.9",
    "@angular/platform-browser-dynamic": "^13.3.9",
    "@angular/router": "^13.3.9",
    "bootstrap": "^4.6.0",
    "core-js": "^3.18.0",
    "popper.js": "^1.16.0",
    "rxjs": "^6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "^13.3.9",
    "@angular/language-service": "^13.3.9",
    "@ngneat/spectator": "^8.0.4",
    "@types/jasmine": "~3.9.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.9.6",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.1",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "standard-changelog": "^2.0.15",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.0",
    "typescript": "4.6.4",
    "watch": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/w11k/angular-sticky-things"
  }
}
