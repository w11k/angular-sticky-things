{
  "name": "angular-sticky-things-demo",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "prestart": "npm run build:lib",
    "build": "ng build --prod --base-href /angular-sticky-things/",
    "build:lib": "ng build angular-sticky-things --prod",
    "test": "ng test angular-sticky-things-demo --watch=false",
    "test:lib": "ng test angular-sticky-things --watch=false",
    "test:lib:watch": "ng test angular-sticky-things",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-watch": "concurrently --kill-others \"npm start\" \"npm run e2e-watch-no-serve\"",
    "e2e-watch-no-serve": "watch \"npm run e2e -- --dev-server-target=\" src e2e --wait=1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.6",
    "@angular/common": "^10.0.6",
    "@angular/compiler": "^10.0.6",
    "@angular/core": "^10.0.6",
    "@angular/forms": "^10.0.6",
    "@angular/platform-browser": "^10.0.6",
    "@angular/platform-browser-dynamic": "^10.0.6",
    "@angular/router": "^10.0.6",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.10",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular-devkit/build-ng-packagr": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "^10.0.6",
    "@angular/language-service": "^10.0.6",
    "@ngneat/spectator": "^4.6.0",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "standard-changelog": "^2.0.15",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "3.9.7",
    "watch": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/w11k/angular-sticky-things"
  }
}
